_G.InfJump_Tunn = not _G.InfJump_Tunn
if _G.InfJump_Tunn then print("Tún nhảy lên mây!") else print("Tún tiếp đất!") end
game:GetService("UserInputService").JumpRequest:Connect(function()
    if _G.InfJump_Tunn then game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping") end
end)
